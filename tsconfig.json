{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "types": ["jest", "node"],
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": [".dumi/tmp/*"],
      "react-data-table": ["src/index.ts"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "**/*.ts", "**/*.tsx"]
}
